<Window x:Class="Keto.Views.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Strona główna" WindowState="Maximized">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Przepisy" Width="200" Height="50" Click="OpenRecipesPage_Click" Margin="10"/>
            <Button Content="Jadłospis" Width="200" Height="50" Click="OpenMealPlanPage_Click" Margin="10"/>
            <Button Content="Kalkulatory" Width="200" Height="50" Click="OpenCalculatorsPage_Click" Margin="10"/>
            <Button Content="Podstawowe informacje o Keto" Width="200" Height="50" Click="OpenInfoPage_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
